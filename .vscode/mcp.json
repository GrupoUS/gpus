{
	"servers": {
		"serena": {
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant"
			],
			"type": "stdio"
		},
		"MCP_DOCKER": {
			"type": "stdio",
			"command": "docker",
			"args": ["mcp", "gateway", "run"]
		}
	}
}
